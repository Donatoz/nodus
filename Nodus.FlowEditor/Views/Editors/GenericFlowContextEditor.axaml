<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vmc="clr-namespace:FlowEditor.ViewModels.Contexts"
             xmlns:dsc="clr-namespace:Nodus.Core.ObjectDescription;assembly=Nodus.Core"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="FlowEditor.Views.Editors.GenericFlowContextEditor"
             x:DataType="vmc:GenericFlowContextViewModel" 
             ClipToBounds="False">
    
    <ItemsControl ClipToBounds="False" ItemsSource="{Binding Editors.Value}">
        <ItemsControl.ItemsPanel>
            <ItemsPanelTemplate>
                <StackPanel/>
            </ItemsPanelTemplate>
        </ItemsControl.ItemsPanel>
        <ItemsControl.ItemTemplate>
            <DataTemplate>
                <dsc:PropertyEditor/>
            </DataTemplate>
        </ItemsControl.ItemTemplate>
    </ItemsControl>
    
</UserControl>
