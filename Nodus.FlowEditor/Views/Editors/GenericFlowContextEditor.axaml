<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vmc="clr-namespace:FlowEditor.ViewModels.Contexts"
             xmlns:pgc="clr-namespace:Avalonia.PropertyGrid.Controls;assembly=Avalonia.PropertyGrid"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="FlowEditor.Views.Editors.GenericFlowContextEditor"
             x:DataType="vmc:GenericFlowContextViewModel" 
             ClipToBounds="False">
    
    <pgc:PropertyGrid ClipToBounds="False" Width="400" DataContext="{Binding DescribedContext.Value}"/>
    
</UserControl>
